<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link type="text/css" rel="stylesheet" href="~/Content/bootstrap-table.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/css/product.css" />
</head>
<body>
    <div>
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script type="text/javascript" src="~/Scripts/utility/utility.js"></script>
    <script type="text/javascript" src="~/Scripts/Service/service.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
